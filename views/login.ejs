<html>
    <head>
        <title>nodejs passport </title>
    </head>
    <body>
        <form action="/login" method="post">
            <div>
                <label>Username:</label>
                <input type="text" name="email"/>
            </div>
            <div>
                <label>Password:</label>
                <input type="password" name="passwd"/>
            </div>
            <div>
                <input type="submit" value="Log In"/>
            </div>
            <p>
                <% if (message) { %>
                    <p><%= message %></p>
                <% } %>

            </p>
        </form>
    </body>
</html>